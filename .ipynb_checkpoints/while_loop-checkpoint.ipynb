    "n = int(input())\n",
    "p = 1\n",
    "\n",
    "i = 1\n",
    "if i <= n:\n",
    "    while True:\n",
    "        a = float(input())\n",
    "        p = p * a\n",
    "        \n",
    "        i += 1\n",
    "        if i > n: break\n",
    "            \n",
    "print(p)"
